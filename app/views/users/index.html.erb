<table border = "1" width = "100%">
   <h2>Users List </h2>
   <tr>
      <td>
         <table border = "1" width = "100%">
            <tr>

              <th>Name</th>
              <th>Created</th>
            </tr>
            <% @users.each do |user| %>
            <tr>
              <td><%= user.name %></td>
              <td><%= format_datetime user.created_at %></td>
            </tr>
            <% end %>
         </table>
      </td>
   </tr>
</table>
